sp <- data %>%
ggplot(aes(data.llp, fm, color = location)) +
geom_point(aes(fill=data.llp),size=3) +
geom_line(aes(group = location),color="black")
sp + scale_color_manual(values=c("green", "blue", "orange", "purple"))
